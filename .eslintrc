{
  "extends": "rallycoding",
  "rules": {
    "arrow-body-style": 0,
    "global-require": 0,
    "import/no-named-as-default-member": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "no-confusing-arrow": 0,
    "object-shorthand": 0,
    "import/newline-after-import": 0,
    "brace-style": 0,
    "camelcase": 0,
    "no-var": 0,
    "max-len": 0,
    "vars-on-top": 0,
  },
  "globals": {
    "fetch": true,
    "alert": true,
    "null": true,
    "WebSocket": true,
    }
}
